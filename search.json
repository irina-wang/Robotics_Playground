---
---
[
  {% for item in site.data.all_data %}
    {% assign temp_level = item.Levels | downcase | append: ', ' %}
    {% assign temp_theme = item.Themes | downcase | append: ', ' %}
    {% assign temp_skill = item.Skills | downcase | append: ', ' %}
    {% assign temp_platform = item.Platforms | downcase | split:" " | join: "_" | prepend: '/' | append: '/' %}
    {% assign temp = temp_level | append: temp_theme | append: temp_skill %}
    {
      "title"    : "{{ item.Names | strip_html | escape }}",
      "tags"     : "{{ temp | split:',' }}",
      "description": "{{ item.Details }}",
      "url": "{{ site.baseurl }}{{ item.Urls}}"
    } {% unless forloop.last %},{% endunless %}
  {% endfor %}
]



